header
	h1 Street Viewer
	form#region-form(name="region-stops", method="post")
		span
			label city: 
			select#region(name="region")
				each region in regions
					option(value="#{region}") #{region}
		span
			label stop count:
			select#stopCount(name="stopcount")
				- var stopCounts = [30, 40, 50, 60, 70, 80, 90, 100]
				each count in stopCounts
					option(value="#{count}") #{count}
		input#submit(type="submit", value="Submit")
			